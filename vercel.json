{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd packages/database && pnpm prisma generate && cd ../../services/ash-admin && pnpm build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "outputDirectory": "services/ash-admin/.next",
  "framework": "nextjs",
  "regions": ["sin1"],
  "build": {
    "env": {
      "HUSKY": "0",
      "SKIP_ENV_VALIDATION": "1"
    }
  }
}
