    "jszip": "^3.10.1",
    "pdf-lib": "^1.17.1",
    "mime-types": "^3.0.1"
  },
  "devDependencies": {
    "@ash-ai/typescript-config": "workspace:*",
    "tsup": "^8.0.1",
