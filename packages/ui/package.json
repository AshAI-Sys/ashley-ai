{
  "name": "@ash-ai/ui",
  "version": "0.0.0",
  "private": true,
  "description": "Shared UI components for ASH AI applications",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./styles": {
      "default": "./src/styles/globals.css"
    },
    "./card": {
      "types": "./src/components/card.tsx",
      "default": "./src/components/card.tsx"
    },
    "./button": {
      "types": "./src/components/button.tsx",
      "default": "./src/components/button.tsx"
    },
    "./badge": {
      "types": "./src/components/badge.tsx",
      "default": "./src/components/badge.tsx"
    },
    "./separator": {
      "types": "./src/components/separator.tsx",
      "default": "./src/components/separator.tsx"
    },
    "./textarea": {
      "types": "./src/components/textarea.tsx",
      "default": "./src/components/textarea.tsx"
    },
    "./input": {
      "types": "./src/components/input.tsx",
      "default": "./src/components/input.tsx"
    },
    "./label": {
      "types": "./src/components/label.tsx",
      "default": "./src/components/label.tsx"
    },
    "./progress": {
      "types": "./src/components/progress.tsx",
      "default": "./src/components/progress.tsx"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist .turbo",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.376.0",
    "react": "^18.2.0",
    "react-day-picker": "^9.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "tailwind-merge": "^2.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ash-ai/eslint-config": "workspace:*",
    "@ash-ai/typescript-config": "workspace:*",
    "@types/bcryptjs": "^3.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
