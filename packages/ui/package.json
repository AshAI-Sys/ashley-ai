  "devDependencies": {
    "@ash-ai/eslint-config": "workspace:*",
    "@ash-ai/typescript-config": "workspace:*",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.0"
