{
  "name": "@ash-ai/ui",
  "version": "0.0.0",
  "private": true,
  "description": "Shared UI components for ASH AI applications",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./styles": {
      "default": "./src/styles/globals.css"
    }
  },
  "scripts": {
