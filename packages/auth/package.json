{
  "name": "@ash-ai/auth",
  "version": "0.0.0",
  "private": true,
  "description": "Authentication and authorization utilities for ASH AI",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "default": "./src/server.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist .turbo",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "next-auth": "^4.24.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.4",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@ash-ai/database": "workspace:*",
    "@ash-ai/typescript-config": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "@ash-ai/database": "workspace:*"
  }
}
